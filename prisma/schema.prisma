
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Coupon {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  title      String
  couponCode String
  expiryDate DateTime
  isActive   Boolean
 
}

model Banner {
    id         String   @id @default(auto()) @map("_id") @db.ObjectId
title String
link String
imageUrl String
isActive Boolean
 createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
